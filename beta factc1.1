                vtot5,vtot6,vtot7,subtotal,vtotal,imp = 0;
        String sT,aux;
        pro1 = 0;
        pro1 = Double.parseDouble(art1.getText()) ;
        pro2 = Double.parseDouble(art2.getText()) ;
        pro3 = Double.parseDouble(art3.getText()) ;
        pro4 = Double.parseDouble(art4.getText()) ;
        pro5 = Double.parseDouble(art5.getText()) ;
        pro6 = Double.parseDouble(art6.getText()) ;
        pro7 = Double.parseDouble(art7.getText()) ;
        vpro1 = Double.parseDouble(precio1.getText()) ;
        vpro2 = Double.parseDouble(precio7.getText()) ;
        vpro3 = Double.parseDouble(precio8.getText()) ;
        vpro4 = Double.parseDouble(precio9.getText()) ;
        vpro5 = Double.parseDouble(precio10.getText()) ;
        vpro6 = Double.parseDouble(precio11.getText()) ;
        vpro7 = Double.parseDouble(precio12.getText()) ;
        vtot1 = pro1*vpro1;
        vtot2 = pro2*vpro2;
        vtot3 = pro3*vpro3;
        vtot4 = pro4*vpro4;
        vtot5 = pro5*vpro5;
        vtot6 = pro6*vpro6;
        vtot7 = pro7*vpro7;
        aux = String.valueOf(vtot1);
        preciotot1.setText(aux);
        aux = String.valueOf(vtot2);
        preciotot7.setText(aux);
        aux = String.valueOf(vtot3);
        preciotot8.setText(aux);
        aux = String.valueOf(vtot4);
        preciotot9.setText(aux);
        aux = String.valueOf(vtot5);
        preciotot10.setText(aux);
        aux = String.valueOf(vtot6);
        preciotot11.setText(aux);
        aux = String.valueOf(vtot7);
        preciotot12.setText(aux);
      
        
        subtotal =vtot1+vtot2+vtot3+vtot4+vtot5+vtot6+vtot7;
        sT=String.valueOf(subtotal);
        subtot.setText(sT);
        imp = subtotal*0.15;
        aux = String.valueOf(imp);
        isv.setText(aux);
        vtotal = imp + subtotal;
        aux = String.valueOf(vtotal);
        totcobro.setText(aux);
        
    }                                                

